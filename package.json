{
    "name": "matrix-skype-bridge",
    "version": "1.0.0",
    "description": "Matrix<->Skype user-puppeting portal",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "lint": "eslint .",
        "test": "NODE_ENV=test nyc mocha test/* --exit",
        "report": "NODE_ENV=test nyc report --reporter=html mocha test/* --exit",
        "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/grigori-gru/matix-skype-bridge.git"
    },
    "keywords": [
        "Skype",
        "Matrix",
        "Bridge"
    ],
    "author": "grigori-gru",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/grigori-gru/matix-skype-bridge/issues"
    },
    "homepage": "https://github.com/grigori-gru/matix-skype-bridge#readme",
    "pre-commit": [
        "lint",
        "test"
    ],
    "dependencies": {
        "tmp-promise": "1.0.4",
        "readline-sync": "1.4.9",
        "matrix-appservice-bridge": "1.4.0",
        "concat-stream": "1.6.2",
        "html-entities": "1.2.1",
        "mime-types": "2.1.18",
        "needle": "2.2.1",
        "node-fetch": "2.1.2",
        "skype-http": "0.0.15",
        "striptags": "3.1.1",
        "winston": "2.4.2",
        "winston-daily-rotate-file": "3.2.1"
    },
    "devDependencies": {
        "chai": "4.1.2",
        "chai-http": "4.0.0",
        "eslint": "4.19.1",
        "eslint-config-google": "0.9.1",
        "eslint-plugin-chai-friendly": "0.4.1",
        "mocha": "5.2.0",
        "nock": "9.2.6",
        "nyc": "11.8.0",
        "pre-commit": "1.2.2",
        "proxyquire": "2.0.1",
        "sinon": "5.0.7",
        "sinon-chai": "3.1.0"
    }
}
